{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DATABASE_URL};Database=${DATABASE_NAME};Username=${DATABASE_USER};Password=${DATABASE_PASSWORD};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000",
      "http://localhost:5173",
      "https://localhost:5173",
      "https://quickcrm.vercel.app",
      "https://*.vercel.app"
    ]
  },
  "Cache": {
    "DefaultExpirationMinutes": 5
  }
}